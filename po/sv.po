msgstr "_Ja"

#: agent/call-pinentry.c:559
msgid "|pinentry-label|_No"
msgstr "_Nej"

#: agent/call-pinentry.c:560
msgid "|pinentry-label|PIN:"
msgstr "PIN-kod:"

